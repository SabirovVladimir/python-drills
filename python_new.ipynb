   "source": [
    "n = 3\n",
    "def pascal(n):\n",
    "    row = [1]\n",
    "    for k in range(1, n + 1):\n",
    "        row.append(row[-1] * (n - k + 1) // k)\n",
    "    return row\n",
    "\n",
    "print(pascal(n))"
   ]
